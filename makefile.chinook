default: build test

clean:
	R -f removeInstalledOncoscapePackages.R

build:
	R CMD install dataPackages/SubjectHistory
	R CMD install dataPackages/Dataset
	R CMD install dataPackages/DEMOdz
	R CMD install dataPackages/TCGAbrain
	R CMD install dataPackages/Groups
	R CMD install dataPackages/ChinookDataset
	R CMD install Oncoscape
	R CMD install analysisPackages/ChinookAnalysis
	R CMD install analysisPackages/chinookAnalysisDemo/SimpleSummaryStats/
	R CMD install analysisPackages/chinookAnalysisDemo/ChinookSimpleSummaryStats
	R CMD install analysisPackages/PCA
	R CMD install analysisPackages/ChinookPCA

test:
	R -f dataPackages/DEMOdz/inst/unitTests/test_DEMOdz.R
	R -f dataPackages/Dataset/inst/unitTests/test_Dataset.R
	R -f dataPackages/TCGAbrain/inst/unitTests/test_TCGAbrain.R
	R -f dataPackages/Groups/inst/unitTests/test_Groups.R
	(cd Oncoscape/inst/unitTests; make)
	R -f analysisPackages/ChinookAnalysis/inst/unitTests/test_ChinookAnalysis.R
	R -f analysisPackages/chinookAnalysisDemo/SimpleSummaryStats/inst/unitTests/test_SimpleSummaryStats.R
	R -f analysisPackages/chinookAnalysisDemo/ChinookSimpleSummaryStats/inst/unitTests/test_ChinookSimpleSummaryStats.R
	(cd analysisPackages/chinookAnalysisDemo/ChinookSimpleSummaryStats/inst/unitTests; make)
	R -f analysisPackages/PCA/inst/unitTests/test_PCA.R
	R -f dataPackages/ChinookDataset/inst/unitTests/test_ChinookDataset.R
	(cd dataPackages/ChinookDataset/inst/unitTests; make)
	(cd analysisPackages/ChinookPCA/inst/unitTests; make)
