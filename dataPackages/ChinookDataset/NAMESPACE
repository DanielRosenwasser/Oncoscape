exportClasses(ChinookDataset)

exportMethods (
  getName,
  getDataset,
  setServer,
  getServer,
  registerMessageHandlers
  )

export(
   ChinookDataset,
   Dataset.getItemNames,
   Dataset.getItemByName,
   Dataset.getManifest,
   Dataset.getDataFrame,
   Dataset.getJSON,
   Dataset.getSubjectHistoryTable,
   Dataset.getNetwork    
   )

import(Dataset)
importClassesFrom(methods, character, integer)
importFrom(methods, new)

